        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
        "bootstrap-vue": "^2.0.0-rc.11",
        "datatables.net": "^1.11.3",
        "datatables.net-dt": "^1.10.19",
        "select2": "^4.0.6-rc.1",
